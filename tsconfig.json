{
    "compilerOptions": {
        "target": "es2018",
        "lib": ["dom", "dom.iterable", "esnext"],
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": true,
        "forceConsistentCasingInFileNames": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "noUnusedLocals": true,
        "noEmit": true
    },
    // "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
